// Decision variables:
ImageTranslation = intVar(0..1);
Cost = intVar(0..500);
TranslationTime = intVar(0..100);

// Constraints:
ImageTranslation == 0 => TranslationTime <= 1;
ImageTranslation == 1 => TranslationTime <= 2;
ImageTranslation == 1;
TranslationTime > 2;

parameters {
  SearchType = DepthFirst;
}